{
  "name": "mstable-shared-data-access",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/mstable/shared/data-access/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/mstable/shared/data-access"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/mstable/shared/data-access/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["yarn nx run mstable-shared-data-access:codegen-graphql"],
        "parallel": false
      }
    },
    "codegen-graphql": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn graphql-codegen --config 'libs/mstable/shared/data-access/codegen.js'"
        ]
      }
    }
  }
}
