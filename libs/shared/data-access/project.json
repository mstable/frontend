{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data-access/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/data-access",
        "tsConfig": "libs/shared/data-access/tsconfig.lib.json",
        "project": "libs/shared/data-access/package.json",
        "entryFile": "libs/shared/data-access/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/shared/data-access/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/data-access/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["yarn nx run shared-data-access:codegen-graphql"],
        "parallel": false
      }
    },
    "codegen-graphql": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn graphql-codegen --config 'libs/shared/data-access/codegen.js'"
        ]
      }
    }
  }
}
