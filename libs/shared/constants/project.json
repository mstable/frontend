{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/constants/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "inputs": [
        { "env": "NX_THE_GRAPH_MV_MAINNET_URL" },
        { "env": "NX_THE_GRAPH_MV_GOERLI_URL" }
      ],
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/constants",
        "tsConfig": "libs/shared/constants/tsconfig.lib.json",
        "project": "libs/shared/constants/package.json",
        "entryFile": "libs/shared/constants/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "rollup.config.js",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/shared/constants/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/constants/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "i18n-extract": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn run formatjs extract 'libs/shared/constants/**/*.{ts,tsx}' --out-file i18n-extractions/libs-shared-constants.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'"
        ]
      }
    }
  }
}
