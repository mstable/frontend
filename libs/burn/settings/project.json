{
  "name": "burn-settings",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/burn/settings/src",
  "projectType": "library",
  "tags": ["burn"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/burn/settings/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "i18n-extract": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn run formatjs extract 'libs/burn/settings/src/**/*.{ts,tsx}' --out-file i18n-extractions/libs-burn-settings.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'"
        ]
      }
    }
  }
}
