{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "strict": false,
    "noPropertyAccessFromIndexSignature": false,
    "paths": {
      "@frontend/mstable-footer": ["libs/mstable/footer/src/index.ts"],
      "@frontend/mstable-metavault": ["libs/mstable/meta-vault/src/index.ts"],
      "@frontend/mstable-settings": ["libs/mstable/settings/src/index.ts"],
      "@frontend/mstable-topnav": ["libs/mstable/topnav/src/index.ts"],
      "@frontend/shared-constants": ["libs/shared/constants/src/index.ts"],
      "@frontend/shared-data-access": ["libs/shared/data-access/src/index.ts"],
      "@frontend/shared-i18n": ["libs/shared/i18n/src/index.ts"],
      "@frontend/shared-icons": ["libs/shared/icons/src/index.ts"],
      "@frontend/shared-modals": ["libs/shared/modals/src/index.ts"],
      "@frontend/shared-notifications": [
        "libs/shared/notifications/src/index.ts"
      ],
      "@frontend/shared-prices": ["libs/shared/prices/src/index.ts"],
      "@frontend/shared-theme": ["libs/shared/theme/src/index.ts"],
      "@frontend/shared-ui": ["libs/shared/ui/src/index.ts"],
      "@frontend/shared-utils": ["libs/shared/utils/src/index.ts"],
      "@frontend/shared-wagmi": ["libs/shared/wagmi/src/index.ts"],
      "@frontend/tools-nx-plugin": ["libs/tools/nx-plugin/src/index.ts"],
      "@frontend/tools-storybook": ["libs/tools/storybook/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
